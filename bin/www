#!/usr/bin/env node

/**
 * Module dependencies.
 */

var app = require("../app");
var debug = require("debug")("howmuchismypension:server");
var http = require("http");

/**
 * Get port from environment and store in Express.
 */
var port = (parseInt(process.env.PORT) || 8080);

/**
 * Create HTTP server.
 */


app.listen(port);